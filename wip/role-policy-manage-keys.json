{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
              "ec2:CreateKeyPair",
              "ec2:DeleteKeyPair",
              "ec2:ImportKeyPair"
            ],
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action": [
              "ec2:DescribeKeyPairs"
            ],
            "Resource": "*"
        }
    ]
}
